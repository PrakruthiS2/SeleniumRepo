<?xml version="1.0" encoding="UTF-8"?>


<suite name="TestNG Regression Suite" verbose="3" parallel="tests"
	thread-count="3">


	<test name="Google Test _ chrome">
		<classes>
			<class name="tests.GoogleTest" />
			<parameter name="browser" value="chrome" />
			<parameter name="url" value="https://google.com" />

		</classes>
	</test>
<test name="Open Cart Test">
		<parameter name="browser" value="firefox" />
		<parameter name="url"
			value="https://naveenautomationlabs.com/opencart/index.php?route=account/login" />


		<classes>
			<class name="mytests.OpenCartLoginTest" />
		</classes>
	</test>


	<test name="OrangeHRM Test">
		<parameter name="browser" value="edge" />
		<parameter name="url"
			value="https://www.orangehrm.com/30-day-free-trial/" />


		<classes>
			<class name="mytests.OrangeHRMTest" />
		</classes>
	</test>



	<test name="Open Cart App Test">
		<parameter name="browser" value="chrome" />
		<parameter name="url"
			value="https://naveenautomationlabs.com/opencart/index.php?route=account/register" />

		<classes>
			<class name="mytests.RegisterPageTest" />
		</classes>
	</test>

</suite>